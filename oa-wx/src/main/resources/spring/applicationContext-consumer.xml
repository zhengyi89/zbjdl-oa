	<dubbo:reference id="bussDemandService" interface="com.zbjdl.oa.service.BussDemandService" group="${service-group}" version="${service-version}" check="false" />
	
	<dubbo:reference id="userinfoService" interface="com.zbjdl.oa.service.UserinfoService" group="${service-group}" version="${service-version}" check="false" />

	<dubbo:reference id="wfTaskInfoService" interface="com.zbjdl.oa.service.WfTaskInfoService" group="${service-group}" version="${service-version}" check="false" />
	
	<dubbo:reference id="smsInfoService" interface="com.zbjdl.oa.service.SmsInfoService" group="${service-group}" version="${service-version}" check="false" />
	
	<dubbo:reference id="wfTaskProcessService" interface="com.zbjdl.oa.service.WfTaskProcessService" group="${service-group}" version="${service-version}" check="false" />
	
	<dubbo:reference id="securityConfigFacade" interface="com.zbjdl.boss.admin.facade.SecurityConfigFacade" group="hengbao-service-group-ransomfloor" version="${service-version}" check="false" />
	
	<dubbo:reference id="noticeService" interface="com.zbjdl.oa.service.NoticeService" group="${service-group}" version="${service-version}" check="false" />
	
